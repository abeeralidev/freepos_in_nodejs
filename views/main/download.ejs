<% layout('./layout') -%>
<style>
@font-face {
	 font-family: "ubuntu";
	 font-style: italic;
	 font-weight: 300;
	 src: local("Lato Light Italic"), local("Lato-LightItalic"), url(https://fonts.gstatic.com/s/ubuntucondensed/v8/u-4k0rCzjgs5J7oXnJcM_0kACGMtT-Dfqw.woff2) format("woff2");
}

 a {
	 text-decoration: none;
	 color: #9ca0b1;
}
 .wrapper {
	 text-align: center;
}
 .wrapper h1 {
	 color: #fff;
	 font-size: 92px;
	 font-family: "ubuntu";
	 text-transform: uppercase;
	 font-weight: 700;
	 font-family: "Josefin Sans", sans-serif;
	 background: linear-gradient(to right, #095fab 10%, #25abe8 50%, #57d75b 60%);
	 background-size: auto auto;
	 background-clip: border-box;
	 background-size: 200% auto;
	 color: #fff;
	 background-clip: text;
	 text-fill-color: transparent;
	 -webkit-background-clip: text;
	 -webkit-text-fill-color: transparent;
	 animation: textclip 1.5s linear infinite;
	 display: inline-block;
}
 .wrapper p {
	 color: #fff;
	 font-family: "ubuntu";
	 text-transform: uppercase;
	 font-family: "Josefin Sans", sans-serif;
	 background: linear-gradient(to right, #095fab 10%, #25abe8 50%, #57d75b 60%);
	 background-size: auto auto;
	 background-clip: border-box;
	 background-size: 200% auto;
	 color: #fff;
	 background-clip: text;
	 text-fill-color: transparent;
	 -webkit-background-clip: text;
	 -webkit-text-fill-color: transparent;
	 animation: textclip 1.5s linear infinite;
	 display: inline-block;
}
 @keyframes textclip {
	 to {
		 background-position: 200% center;
	}
}


</style>
<main id="content" role="main">
    <!-- Hero Section -->
    <div class="gradient-y-sm-primary position-relative z-index-2 overflow-hidden">
        <div class="container space-top-2 space-top-lg-4 position-relative">
            <!-- Title -->
            <div class="w-md-80 w-lg-50 text-center mx-md-auto mb-5 mb-md-9">
                <div class="wrapper">
                    <h1 class="display-4 mb-3 mainheading">FreePOS</h1>
                    <p class="lead">FreePOS is the free point of sales, inventory management, and accounting software. FreePOS decreases the gaps between customers to sellers. `Lets go over the horizon`</p>
                </div>
                <a href="#downoadlink">
                  <button class="btn btn-primary">Download</button>
                </a>
            </div>
        </div>

        <!-- SVG Bottom Shape -->
        <!-- <figure class="position-absolute right-0 bottom-0 left-0">
            <svg preserveAspectRatio="none" xmlns="http://www.w3.org/2000/svg" x="0px" y="0px"
                viewBox="0 0 1921 273">
                <polygon fill="#fff" points="0,273 1921,273 1921,0 " />
            </svg>
        </figure> -->
        <!-- End SVG Bottom Shape -->
    </div>
    <!-- End Hero Section -->

    <!-- Features Section -->

    <div class="container">

    <div class="row">
      <div class="col-4">
        <div class="row">

          <p>
            
          <button class="btn btn-sm btn-primary" type="button" data-bs-toggle="collapse" data-bs-target="#systemreqirements" aria-expanded="false" aria-controls="systemreqirements">System Requirements</button>
        </p>
        <div class="row">
          <div class="col">
            <div class="collapse multi-collapse" id="systemreqirements">
              <div class="card card-body">
                FreePos Is very Lightweight that can install in low-ended Pc
                <ul>
                  <li>Operating System : <br> Windows 7 Windows 8,10,11 or heigher </li>
                  <li>Ram : Min 2GB </li>
                  <li>Storage : Min 500MB </li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
        <div class="row">

          <p>
            
          <button class="btn btn-sm btn-primary" type="button" data-bs-toggle="collapse" data-bs-target="#downloadinstructions" aria-expanded="false" aria-controls="downloadinstructions">Download Instructions</button>
        </p>
        <div class="row">
          <div class="col">
            <div class="collapse multi-collapse" id="downloadinstructions">
              <div class="card card-body">
                Please watch this video it will show you how to install FreePos in your PC
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
      <div class="col-8">
        <iframe width="100%" height="411px" src="https://www.youtube.com/embed/UrhOueko31M" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        
      </div>
    </div>
  </div>

    <!-- End Features Section -->

    <!-- CTA Section -->
    <div class="container">
        <div class="w-lg-85 mx-lg-auto">
            <div class="card overflow-hidden p-4">
                <div class="row justify-content-md-start align-items-md-center text-center text-md-left">
                    <div class="col-md-5 offset-md-3 mb-3 mb-md-0">
                        <h3 class="h4 mb-0">Revolutionizing the way your team works!</h3>
                    </div>
                    <div class="col-md-4 text-md-right">
                        <a class="btn btn-primary transition-3d-hover" href="#">Get Started</a>
                    </div>
                </div>

                <!-- SVG Component -->
                <figure class="w-25 d-none d-md-block position-absolute top-0 left-0 mt-n2">
                    <img class="img-fluid" src="./assets/svg/illustrations/apps.svg" alt="Image Description">
                </figure>
                <!-- End SVG Component -->
            </div>
        </div>


      
      <div class="row mb-10" id="downoadlink">
      <div class="col-12">
        <div class="row">
          <div class="col-12 text-center ">
            <img src="https://www.candelarms.com/wp-content/uploads/2019/01/product-illustration-2.png" alt="">
          </div>
        </div>
        <div class="row">
          <div class="col-12 text-center ">
            <h2>Download Setup for 32 Bit Or 64 Bit</h2>
            <p>
              <b>

                (this setup contains all files including database server, sample database, and Dot Net framework.)
              </b>
<br>
By downloading this software, you agree to our <a class="text-primary" href="#"> License Agreement </a> and <a class="text-primary" href="#">

 Privacy Policy
</a> 
            </p>
            <button class="btn btn-sm btn-success" onclick="navigatetolink('github.com/ravicosoftltd/FreePOS-wpf/releases/latest/download/FreePOS.Setup.msi')"> Download FreePos</button>
          </div>
        </div>
      </div>
    </div>
  </div>
    




    <div class="bg-dark">
        <div class="container space-top-2">
            <div class="row justify-content-md-between mb-6">
                <div class="col-md-6 col-lg-5 mb-3 mb-md-0">
                    <h2 class="text-white font-weight-normal">Subscribe to get the latest from FreePos</h2>
                </div>

                <!-- Form -->
                <div class="col-md-6">
                    <form class="js-validate mb-2">
                        <label class="sr-only" for="signupSrEmail">Email</label>
                        <div class="input-group">
                            <input type="email" class="form-control" name="email" id="signupSrEmail"
                                placeholder="Your email address" aria-label="Your email address" required>
                            <div class="input-group-append">
                                <button type="submit" class="btn btn-block btn-primary">Subscribe</button>
                            </div>
                        </div>
                    </form>
                    <!-- End Form -->

                    <p class="small text-white-70">You can unsubscribe at any time.</p>
                </div>
            </div>

            <hr class="opacity-xs my-0">
        </div>
    </div>
    <!-- End Stories Section -->
</main>


<script>
    function navigatetopageblank(link){
        window.open("/"+link,"_blank")

    }
    function navigatetopage(link){
        location.replace("/"+link)

    }
    function navigatetolinkblank(link){
        window.open("https://"+link)

    }
    function navigatetolink(link){
        location.replace("https://"+link)

    }
</script>
